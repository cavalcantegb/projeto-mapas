os, desta vez para  homenagear uma santa e assim surgiu o reisado santa Helena.  para esse ano eu vou pro Rio de janeiro trabalhei o ano inteiro na  minha profiss\u00e3o Catei latinha, ferro velho e papel\u00e3o ensaiei o meu reisado pra dan\u00e7ar no meu sert\u00e3o.","createTimestamp":{"date":"2018-12-06 09:17:50.000000","timezone_type":3,"timezone":"America\/Fortaleza"},"status":1,"userId":23153,"updateTimestamp":{"date":"2019-05-27 09:48:28.000000","timezone_type":3,"timezone":"America\/Fortaleza"},"parent":null,"user":23153,"subsite":3,"emailPublico":"reisadosantahelena@gmail.com","geoMesorregiao":"SUL CEARENSE","geoMesorregiao_cod":"SUL CEARENSE","geoMicrorregiao":"CARIRI","geoMicrorregiao_cod":"CARIRI","geoMunicipio":"JUAZEIRO DO NORTE","geoMunicipio_cod":"JUAZEIRO DO NORTE","children":[27835],"spaces":[1805],"projects":[],"ownedOpportunities":[],"events":[],"relatedOpportunities":[],"terms":{"tag":[],"area":["Cultura Popular"]},"type":{"id":1,"name":"Individual"},"subsiteId":null,"nomeCompleto":null,"documento":null,"raca":null,"dataDeNascimento":null,"localizacao":null,"genero":null,"orientacaoSexual":null,"emailPrivado":null,"telefonePublico":null,"telefone1":null,"telefone2":null,"endereco":null,"En_CEP":null,"En_Nome_Logradouro":null,"En_Num":null,"En_Complemento":null,"En_Bairro":null,"En_Municipio":null,"En_Estado":null,"En_Pais":null,"site":null,"facebook":null,"twitter":null,"googleplus":null,"instagram":null,"geoPais":null,"geoPais_cod":null,"geoRegiao":null,"geoRegiao_cod":null,"geoEstado":null,"geoEstado_cod":null,"geoZona":null,"geoZona_cod":null,"geoSubprefeitura":null,"geoSubprefeitura_cod":null,"geoDistrito":null,"geoDistrito_cod":null,"geoSetor_censitario":null,"geoSetor_censitario_cod":null,"opportunityTabName":null,"useOpportunityTab":null,"aldirblanc_inciso1_registration":null,"aldirblanc_inciso2_registration":null,"sentNotification":null,"_children":null,"_spaces":null,"_projects":null,"_ownedOpportunities":null,"_events":null,"_relatedO